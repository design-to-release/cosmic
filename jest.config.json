{
  "rootDir": "src",
  "coverageDirectory": "coverage",
  "testMatch": [
    "<rootDir>/**/*.spec.{ts,tsx,js,jsx}",
    "<rootDir>/*.spec.{ts,tsx,js,jsx}"
  ],
  "transform": {
    "^.+\\.(t|j)sx?$": [
      "@swc/jest"
    ]
  },
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json",
    "node"
  ],
  "testEnvironment": "jsdom"
}