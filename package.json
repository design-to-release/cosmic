{
  "name": "cosmic-design",
  "description": "awesome design tool",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/design-to-release/cosmic.git"
  },
  "author": {
    "name": "qiansc",
    "url": "https://github.com/design-to-release"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/design-to-release/cosmic/issues"
  },
  "keywords": [
    "electron",
    "svelte",
    "typescript",
    "electron-builder",
    "electron-log",
    "tslint",
    "ts-jest",
    "electron-settings",
    "electron-updater",
    "rollup"
  ],
  "homepage": "https://github.com/design-to-release/cosmic#readme",
  "main": "dist/app/index.js",
  "scripts": {
    "prestart": "cross-env NODE_ENV=development node scripts/preBuild.js",
    "start": "cross-env NODE_ENV=development npm-run-all -p -r build:frontend:dev start:electron",
    "prebuild": "cross-env NODE_ENV=production node scripts/preBuild.js",
    "build": "cross-env NODE_ENV=production npm-run-all -s build:frontend:prod build:electron:prod",
    "postbuild": "cross-env NODE_ENV=production node scripts/postBuild.js",
    "build:frontend:dev": "cross-env NODE_ENV=development rollup -c -w",
    "build:frontend:prod": "cross-env NODE_ENV=production rollup -c",
    "build:electron:dev": "tsc -p ./tsconfig.app.json",
    "build:electron:dev:watch": "tsc -w -p ./tsconfig.app.json",
    "build:electron:prod": "cross-env NODE_ENV=production tsc -p ./tsconfig.app.prod.json",
    "validate:svelte": "svelte-check --compiler-warnings \"module-script-reactive-declaration:ignore\"",
    "start:electron:run": "electron .",
    "start:electron:nodemon": "nodemon",
    "start:electron:dev": "npm-run-all -s build:electron:dev start:electron:nodemon",
    "start:electron": "npm-run-all -p build:electron:dev:watch start:electron:dev",
    "test": "jest",
    "postinstall": "electron-builder install-app-deps",
    "preinstall": "npx only-allow pnpm",
    "preinstall:all": "npx only-allow pnpm",
    "prepack": "npm run build",
    "pack": "electron-builder --dir",
    "predist": "npm run build",
    "dist": "cross-env CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder",
    "postdist": "node scripts/cleanBuilds.js",
    "format:check": "prettier --check \"{src,scripts}/**/**/*.{ts,svelte,css,md,json,js}\" \"*.{ts,svelte,css,md,json,js}\"",
    "format": "npm run format:check -- --write",
    "lint:electron": "eslint -c eslint.app.json --ext .js,.ts src/app",
    "lint:svelte": "eslint -c eslint.svelte.json --ext .js,.ts,.svelte src/workbench src/core src/modules",
    "lint:scripts": "eslint --ext .js scripts/",
    "lint": "npm-run-all -s lint:electron lint:svelte lint:scripts",
    "prepublishOnly": "npm-run-all -s test lint format",
    "preversion": "npm-run-all -s lint format",
    "typecheck-app": "tsc --noEmit -p ./tsconfig.app.json",
    "typecheck-svelte": "tsc --noEmit -p ./tsconfig.svelte.json",
    "typecheck": "npm run typecheck-app && npm run typecheck-svelte",
    "storybook": "start-storybook -p 4200 -c .storybook --no-manager-cache",
    "storybook:build": "build-storybook -c .storybook -o dist/docs/components",
    "compodoc": "compodoc -s",
    "compodoc:build": "compodoc"
  },
  "dependencies": {
    "@types/color": "^3.0.1",
    "@urql/svelte": "~1.3.0",
    "canvaskit-wasm": "~0.28.1",
    "color": "^3.1.3",
    "electron-log": "^4.3.1",
    "electron-store": "^7.0.2",
    "electron-updater": "^4.3.5",
    "eventemitter2": "^6.4.4",
    "graphql": "~15.6.0",
    "install": "~0.13.0",
    "inversify": "^5.1.1",
    "keycode": "^2.2.0",
    "reflect-metadata": "^0.1.13",
    "smelte": "^1.1.5",
    "split.js": "^1.6.4",
    "svelte-spa-router": "~3.2.0",
    "tailwindcss-typography": "~3.1.0",
    "ts-mixer": "~6.0.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.14",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "~3.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-controls": "^6.2.9",
    "@storybook/addon-docs": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storysource": "^6.2.9",
    "@storybook/addon-svelte-csf": "^1.0.0",
    "@storybook/addon-viewport": "^6.2.9",
    "@storybook/addons": "^6.2.9",
    "@storybook/source-loader": "^6.2.9",
    "@storybook/svelte": "^6.2.9",
    "@storybook/theming": "^6.2.9",
    "@swc/jest": "^0.1.4",
    "@tsconfig/svelte": "^1.0.13",
    "@types/clean-css": "^4.2.3",
    "@types/html-minifier": "^4.0.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.17.1",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "clean-css": "^5.0.1",
    "copy-webpack-plugin": "6.4",
    "cross-env": "^7.0.3",
    "electron": "^12.0.6",
    "electron-builder": "^22.10.5",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jsdoc": "^32.0.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-svelte3": "^3.2.0",
    "fast-glob": "^3.2.7",
    "html-minifier": "^4.0.0",
    "jest": "^27.0.6",
    "less": "^4.1.1",
    "merge-deep": "^3.0.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.0",
    "postcss-import": "^14.0.2",
    "postcss-loader": "~4.0.4",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.3.1",
    "rollup": "^2.39.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-svg-import": "^1.5.1",
    "rollup-plugin-terser": "^7.0.0",
    "sass": "^1.35.1",
    "sb": "^6.2.9",
    "storybook-dark-mode": "^1.0.8",
    "svelte": "^3.38.1",
    "svelte-check": "~2.2.6",
    "svelte-inline-svg": "^1.0.8",
    "svelte-loader": "^3.1.1",
    "svelte-preprocess": "^4.7.3",
    "svelte-typed-component": "^1.1.1",
    "svelte-waypoint": "^0.1.4",
    "svg-inline-loader": "^0.8.2",
    "tailwindcss": "^2.1.4",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "pre-push": "npm run typecheck && npm run format:check"
  },
  "lint-staged": {
    "src/app/**/*.{js,ts,svelte}": "eslint -c eslint.app.json",
    "src/{core,app,parts,modules,workbench}/**/*.{js,ts,svelte}": "eslint -c eslint.svelte.json",
    "scripts/**/*.js": "eslint"
  },
  "build": {
    "productName": "Electron Svelte Typescript",
    "appId": "electron.svelte.typescript",
    "copyright": "Copyright Â© 2021 ${author}",
    "win": {
      "target": [
        "nsis"
      ]
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "category": "productivity",
      "type": "distribution",
      "hardenedRuntime": "true"
    },
    "linux": {
      "target": [
        "AppImage",
        "snap"
      ],
      "category": "productivity"
    },
    "files": [
      "build/*/",
      "node_modules/*/",
      "package.json"
    ],
    "extraResources": [
      "assets/**"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "design-to-release",
        "repo": "cosmic"
      }
    ]
  }
}
